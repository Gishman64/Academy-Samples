<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxb:bindings version="2.1"
               xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xs="http://www.w3.org/2001/XMLSchema"
               xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd">

    <jaxb:globalBindings
            typesafeEnumMaxMembers="2000"
            collectionType="java.util.LinkedList">

        <xjc:serializable uid="-1"/>
        <jaxb:javaType xmlType="xs:dateTime" name="java.time.LocalDateTime"
                       parseMethod="xml.util.XMLLocalDateConverter.parse"
                       printMethod="xml.util.XMLLocalDateConverter.print"/>
    </jaxb:globalBindings>
</jaxb:bindings>